create table PROJECT_ISSUE_ATTACHMENT
(
  ID               NUMBER(10) not null,
  PROJECT_ISSUE_ID NUMBER(10) not null,
  FILE_NAME        VARCHAR2(255) not null,
  CREATEDON        DATE not null,
  CREATEDBY        VARCHAR2(16) not null,
  NOTES            VARCHAR2(400)
)
;
comment on table PROJECT_ISSUE_ATTACHMENT
  is '(Code: PRJISSATT)';
alter table PROJECT_ISSUE_ATTACHMENT
  add constraint PK_PRJISSATT primary key (ID);
alter table PROJECT_ISSUE_ATTACHMENT
  add constraint FK_PRJISSATT_PRJISSUE foreign key (PROJECT_ISSUE_ID)
  references PROJECT_ISSUE (ID);

