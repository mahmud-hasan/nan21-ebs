create table MENUITEM_PARAM
(
  ID          NUMBER(10) not null,
  MENUITEM_ID NUMBER(10) not null,
  PARAM_NAME  VARCHAR2(32) not null,
  PARAM_VALUE VARCHAR2(255)
)
;
comment on table MENUITEM_PARAM
  is 'Parameters used to call the target GUI from menu (Code: MENUITEMPARAM)';
alter table MENUITEM_PARAM
  add constraint PK_MENUITEMPARAM primary key (ID);

