create table PROJECT_ISSUE_NOTE
(
  ID               NUMBER(10) not null,
  PROJECT_ISSUE_ID NUMBER(10) not null,
  NOTE             VARCHAR2(4000) not null,
  CREATEDBY        VARCHAR2(16) not null,
  CREATEDON        DATE not null,
  MODIFIEDBY       VARCHAR2(16) not null,
  MODIFIEDON       DATE not null
)
;
comment on table PROJECT_ISSUE_NOTE
  is '(Code: PRJISSNOTE)';
alter table PROJECT_ISSUE_NOTE
  add constraint PK_PRJISSNOTE primary key (ID);
alter table PROJECT_ISSUE_NOTE
  add constraint FK_PRJISSNOTE_PRJISSUE foreign key (PROJECT_ISSUE_ID)
  references PROJECT_ISSUE (ID);

