create table DOCUMENT_SERIAL
(
  ID            NUMBER(10) not null,
  CLIENT_ID     NUMBER(10) not null,
  DOCUMENT_TYPE VARCHAR2(16) not null,
  SERIAL        VARCHAR2(16) not null
)
;
comment on table DOCUMENT_SERIAL
  is 'Document serials for all document types (Code: DOCSER)';
comment on column DOCUMENT_SERIAL.DOCUMENT_TYPE
  is 'IINV,..';
alter table DOCUMENT_SERIAL
  add constraint PK_DOCSER primary key (ID);
alter table DOCUMENT_SERIAL
  add constraint UK_DOCSER unique (CLIENT_ID, DOCUMENT_TYPE, SERIAL);

