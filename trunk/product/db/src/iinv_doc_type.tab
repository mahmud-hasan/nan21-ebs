create table IINV_DOC_TYPE
(
  ID                NUMBER(10) not null,
  CODE              VARCHAR2(16) not null,
  NAME              VARCHAR2(64) not null,
  ACTIVE            CHAR(1) default 'N' not null,
  PRINT_REPORT_CODE VARCHAR2(32),
  CLIENT_ID         NUMBER(10) not null,
  CREATEDON         DATE not null,
  CREATEDBY         VARCHAR2(16) not null,
  MODIFIEDON        DATE not null,
  MODIFIEDBY        VARCHAR2(16) not null
)
;
comment on table IINV_DOC_TYPE
  is 'Invoice document types (Code: IINVDOCTYPE)';
alter table IINV_DOC_TYPE
  add constraint PK_IINVDOCTYPE primary key (ID);
alter table IINV_DOC_TYPE
  add constraint UK_IINVDOCTYPE unique (CODE);

