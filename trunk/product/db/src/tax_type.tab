create table TAX_TYPE
(
  CODE      VARCHAR2(16) not null,
  NAME      VARCHAR2(64) not null,
  NOTES     VARCHAR2(255),
  ACTIVE    CHAR(1) not null,
  IS_SYSTEM CHAR(1) not null
)
;
comment on table TAX_TYPE
  is 'Possible tax type definitions (Code: TAXTYPE)';
comment on column TAX_TYPE.IS_SYSTEM
  is 'Specifies if this is a record required by the system to work properly. Cannot be updated/deleted by user. ';
alter table TAX_TYPE
  add constraint PK_TAXTYPE primary key (CODE);

