create table RINV_DOC_TYPE
(
  ID         NUMBER(10) not null,
  CODE       VARCHAR2(16) not null,
  NAME       VARCHAR2(64) not null,
  ACTIVE     CHAR(1) not null,
  CLIENT_ID  NUMBER(10) not null,
  CREATEDON  DATE not null,
  CREATEDBY  VARCHAR2(16) not null,
  MODIFIEDON DATE not null,
  MODIFIEDBY VARCHAR2(16) not null
)
;
comment on table RINV_DOC_TYPE
  is 'Invoice document types (Code: RINVDOCTYPE)';
alter table RINV_DOC_TYPE
  add constraint PK_RINVDOCTYPE primary key (ID);
alter table RINV_DOC_TYPE
  add constraint UK_RINVDOCTYPE unique (CODE);
alter table RINV_DOC_TYPE
  add constraint CHK_RINVDOCTYPE_ACTIVE
  check (ACTIVE IN ('Y','N'));

