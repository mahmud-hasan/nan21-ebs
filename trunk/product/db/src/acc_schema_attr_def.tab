create table ACC_SCHEMA_ATTR_DEF
(
  ID            NUMBER(10) not null,
  PROPERTY_NAME VARCHAR2(32) not null,
  PROPERTY_TYPE VARCHAR2(32),
  DESCRIPTION   VARCHAR2(4000),
  CREATEDON     DATE,
  CREATEDBY     VARCHAR2(16),
  MODIFIEDON    DATE,
  MODIFIEDBY    VARCHAR2(16)
)
;
comment on table ACC_SCHEMA_ATTR_DEF
  is 'Definition of accounting schema attributes. Contains all posible attributes (Code: ACCSCHATTRDEF)';
alter table ACC_SCHEMA_ATTR_DEF
  add constraint PK_ACCSCHATTRDEF primary key (ID);
alter table ACC_SCHEMA_ATTR_DEF
  add constraint UK_ACCSCHATTRDEF unique (PROPERTY_NAME);

