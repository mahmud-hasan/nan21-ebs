create table SYS_USER_DOCSERRNG
(
  ID           NUMBER(10) not null,
  GRANTED_TO   VARCHAR2(16) not null,
  DOCSERRNG_ID NUMBER(10) not null
)
;
comment on table SYS_USER_DOCSERRNG
  is 'Document serial range accesible to be used in billing by a certain user (USRDOCSERRNG)';
alter table SYS_USER_DOCSERRNG
  add constraint PK_USRDOCSERRNG primary key (ID);
alter table SYS_USER_DOCSERRNG
  add constraint UK_USRDOCSERRNG unique (DOCSERRNG_ID, GRANTED_TO);
alter table SYS_USER_DOCSERRNG
  add constraint FK_USRDOCSERRNG_DOCSERRNG foreign key (DOCSERRNG_ID)
  references DOCUMENT_SERIAL_RANGE (ID);

