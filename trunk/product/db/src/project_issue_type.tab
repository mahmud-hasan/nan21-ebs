create table PROJECT_ISSUE_TYPE
(
  ID   NUMBER(10) not null,
  CODE VARCHAR2(16) not null,
  NAME VARCHAR2(64) not null
)
;
comment on table PROJECT_ISSUE_TYPE
  is '(Code: PRJISSTYP)';
alter table PROJECT_ISSUE_TYPE
  add constraint PK_PRJISSTYP primary key (ID);
alter table PROJECT_ISSUE_TYPE
  add constraint UK_PRJISSTYP unique (CODE);

