create table PROJECT_TYPE
(
  ID   NUMBER(10) not null,
  CODE VARCHAR2(16) not null,
  NAME VARCHAR2(64) not null
)
;
comment on table PROJECT_TYPE
  is 'Project types (Code: PRJTYPE)';
alter table PROJECT_TYPE
  add constraint PK_PRJTYPE primary key (ID);
alter table PROJECT_TYPE
  add constraint UK_PRJTYPE unique (CODE);

