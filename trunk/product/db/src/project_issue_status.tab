create table PROJECT_ISSUE_STATUS
(
  ID   NUMBER(10) not null,
  CODE VARCHAR2(16) not null,
  NAME VARCHAR2(64) not null
)
;
comment on table PROJECT_ISSUE_STATUS
  is '(Code: PRJISSSTS)';
alter table PROJECT_ISSUE_STATUS
  add constraint PK_PRJISSSTS primary key (ID);
alter table PROJECT_ISSUE_STATUS
  add constraint UK_PRJISSSTS unique (CODE);

