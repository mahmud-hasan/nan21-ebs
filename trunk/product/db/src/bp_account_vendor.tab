create table BP_ACCOUNT_VENDOR
(
  ID                  NUMBER(10) not null,
  CLIENT_ID           NUMBER(10) not null,
  BPARTNER_ID         NUMBER(10) not null,
  ACCT_PREPAYMENT     VARCHAR2(32),
  ACCT_LIABILITY      VARCHAR2(32),
  ACCT_LIABILITY_SRVC VARCHAR2(32),
  CREATEDON           DATE not null,
  CREATEDBY           VARCHAR2(16) not null,
  MODIFIEDON          DATE not null,
  MODIFIEDBY          VARCHAR2(16) not null,
  ACCSCHEMA_ID        NUMBER(10) not null
)
;
comment on table BP_ACCOUNT_VENDOR
  is 'Vendor accounts of a business partner (Code: BPACCVEND)';
alter table BP_ACCOUNT_VENDOR
  add constraint PK_BPACCVEND primary key (ID);
alter table BP_ACCOUNT_VENDOR
  add constraint UK_BPACCVEND unique (CLIENT_ID, BPARTNER_ID, ACCSCHEMA_ID);

