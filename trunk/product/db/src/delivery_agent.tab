create table DELIVERY_AGENT
(
  ID     NUMBER(10) not null,
  CODE   VARCHAR2(16),
  NAME   VARCHAR2(64),
  ACTIVE CHAR(1)
)
;
comment on table DELIVERY_AGENT
  is 'Delivery agents, who act as delivery couriers (Code: DLVRAGENT)';
alter table DELIVERY_AGENT
  add constraint PK_DLVRAGENT primary key (ID);
alter table DELIVERY_AGENT
  add constraint UK_DLVRAGENT unique (CODE);
alter table DELIVERY_AGENT
  add constraint CHK_DLVRAGENT_ACTIVE
  check (ACTIVE IN ('Y','N'));

