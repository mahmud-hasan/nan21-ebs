create table BILLING_CYCLE_CLASS
(
  CODE       VARCHAR2(16) not null,
  NAME       VARCHAR2(32),
  STARTDAY   NUMBER(2),
  CREATEDON  DATE not null,
  CREATEDBY  VARCHAR2(16) not null,
  MODIFIEDON DATE not null,
  MODIFIEDBY VARCHAR2(16) not null
)
;
comment on table BILLING_CYCLE_CLASS
  is 'Billing cycle class (Code: BILLCYCCLASS)';
comment on column BILLING_CYCLE_CLASS.STARTDAY
  is 'Start day of the billing cycle, the N-th day of the month  ';
alter table BILLING_CYCLE_CLASS
  add constraint PK_BILLCYCCLASS primary key (CODE);

