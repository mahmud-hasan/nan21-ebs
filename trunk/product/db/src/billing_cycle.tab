create table BILLING_CYCLE
(
  ID                NUMBER(10) not null,
  BILLCYCCLASS_CODE VARCHAR2(16) not null,
  CLIENT_ID         NUMBER(10) not null,
  STARTDATE         DATE not null,
  ENDDATE           DATE not null,
  NAME              VARCHAR2(64),
  CLOSED            CHAR(1) default 'N' not null,
  CREATEDON         DATE not null,
  CREATEDBY         VARCHAR2(16) not null,
  MODIFIEDON        DATE not null,
  MODIFIEDBY        VARCHAR2(16) not null
)
;
comment on table BILLING_CYCLE
  is 'Billing cycles (Code: BILLCYC)';
alter table BILLING_CYCLE
  add constraint PK_BILLCYC primary key (ID);

