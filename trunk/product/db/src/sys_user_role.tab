create table SYS_USER_ROLE
(
  ID        NUMBER(10) not null,
  USER_ID   NUMBER(10) not null,
  ROLE_NAME VARCHAR2(16) not null,
  CREATEDON DATE not null,
  CREATEDBY VARCHAR2(16) not null
)
;
comment on table SYS_USER_ROLE
  is 'Roles granted to users (Code: USRROL)';
alter table SYS_USER_ROLE
  add constraint PK_USRROL primary key (ID);
alter table SYS_USER_ROLE
  add constraint UK_USRROL unique (USER_ID, ROLE_NAME);
alter table SYS_USER_ROLE
  add constraint FK_USRROL_ROLE foreign key (ROLE_NAME)
  references SYS_ROLE (NAME) on delete cascade;
alter table SYS_USER_ROLE
  add constraint FK_USRROL_USER foreign key (USER_ID)
  references SYS_USER (ID) on delete cascade;

