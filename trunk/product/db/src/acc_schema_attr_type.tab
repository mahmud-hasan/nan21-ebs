create table ACC_SCHEMA_ATTR_TYPE
(
  ID          NUMBER(10) not null,
  CODE        VARCHAR2(32),
  DESCRIPTION VARCHAR2(400)
)
;
comment on table ACC_SCHEMA_ATTR_TYPE
  is 'Accounting schema attributes type (Code: ACCSCHATTRTYPE)';
alter table ACC_SCHEMA_ATTR_TYPE
  add constraint PK_ACCSCHATTRTYPE primary key (ID);
alter table ACC_SCHEMA_ATTR_TYPE
  add constraint UK_ACCSCHATTRTYPE unique (CODE);

