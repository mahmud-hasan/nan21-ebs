create table UOM_TYPE
(
  ID   NUMBER(10) not null,
  CODE VARCHAR2(16) not null,
  NAME VARCHAR2(64) not null
)
;
comment on table UOM_TYPE
  is 'Measuring units types (Code: UOMTYPE)';
alter table UOM_TYPE
  add constraint PK_UOM_TYPE primary key (ID);
alter table UOM_TYPE
  add constraint UK_UOM_TYPE unique (CODE);

