create table REPORTING_PERIOD
(
  ID NUMBER(10) not null
)
;
comment on table REPORTING_PERIOD
  is 'Custom reporting periods. Can be linked to accounting periods (Code: REPPERIOD)';
alter table REPORTING_PERIOD
  add constraint PK_REPPERIOD primary key (ID);
create unique index PK_REPPER on REPORTING_PERIOD (ID);

