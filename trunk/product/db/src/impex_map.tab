create table IMPEX_MAP
(
  ID          NUMBER(10) not null,
  CODE        VARCHAR2(16) not null,
  DESCRIPTION VARCHAR2(400) not null,
  CREATEDON   DATE not null,
  CREATEDBY   VARCHAR2(16) not null,
  MODIFIEDON  DATE not null,
  MODIFIEDBY  VARCHAR2(16) not null
)
;
comment on table IMPEX_MAP
  is 'Data import-export map (Code: IMPEXMAP)';
alter table IMPEX_MAP
  add constraint PK_IMPEXMAPITEM primary key (ID);
alter table IMPEX_MAP
  add constraint UK_IMPEXMAP unique (CODE);

